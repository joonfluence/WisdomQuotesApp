-- WisdomQuotesApp.Quote__Author definition

CREATE TABLE `Quote__Author` (
  `AuthorId` int unsigned NOT NULL AUTO_INCREMENT,
  `Name` varchar(45) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci NOT NULL COMMENT '작가이름',
  `BornedAt` int unsigned DEFAULT NULL COMMENT '탄생시기',
  `E_Image` varchar(100) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL COMMENT '작가 이미지',
  `DiedAt` int unsigned DEFAULT NULL,
  `Job` varchar(20) DEFAULT NULL COMMENT '직업명',
  `MajorId` int DEFAULT NULL,
  `Twitter` varchar(100) DEFAULT NULL COMMENT '트위터 계정',
  PRIMARY KEY (`AuthorId`),
  KEY `Quote__Author_FK` (`MajorId`),
  CONSTRAINT `Quote__Author_FK` FOREIGN KEY (`MajorId`) REFERENCES `Quote__Author__Major` (`MajorId`)
) ENGINE=InnoDB AUTO_INCREMENT=11 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;